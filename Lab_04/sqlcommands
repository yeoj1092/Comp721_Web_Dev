CREATE TABLE `cars` (
  `car_id` int(11) NOT NULL AUTO_INCREMENT PRIMARY KEY, 
  `make` varchar(18) NOT NULL,
  `model` varchar(18) NOT NULL,
  `price` int(11) NOT NULL,
  `yom` int(11) NOT NULL
) ;

